{
  "functions": {
    "app/api/copilot/advanced-chat/route.ts": {
      "maxDuration": 60,
      "memory": 3008
    },
    "app/api/newspapers/generate/route.ts": {
      "maxDuration": 60,
      "memory": 2048
    },
    "app/api/seo/articles/generate/route.ts": {
      "maxDuration": 45,
      "memory": 2048
    },
    "app/api/articles/rewrite/route.ts": {
      "maxDuration": 45,
      "memory": 2048
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate=3600"
        }
      ]
    }
  ]
}
